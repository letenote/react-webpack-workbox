{
  "name": "react-webpack-workbox",
  "version": "1.0.0",
  "description": "boilerplate react build using webpack and build service worker using workbox",
  "main": "index.js",
  "scripts": {
    "build-sw": "node ./workbox/sw-build.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open",
    "prod": "GENERATE_SOURCEMAP=false cross-env NODE_ENV=production webpack && npm run build-sw",
    "serve": "rm -rf build && npm run prod && serve -s -p 8989 build",
    "svg": "svgr -d src/components/icons src/components/icons/svgs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/letenote/react-webpack-workbox.git"
  },
  "keywords": [],
